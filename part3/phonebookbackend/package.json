    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "response-time": "^2.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
